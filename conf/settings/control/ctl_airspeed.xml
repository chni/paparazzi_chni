<!DOCTYPE settings SYSTEM "../settings.dtd">

<!-- A conf to use to tune a new A/C with airspeed-->

<settings>
  <dl_settings>
      <dl_settings name="airspeed">
        <dl_setting max="40" min="5" step="0.1" var="v_ctl_auto_airspeed_setpoint" shortname="as_sp" param="V_CTL_AUTO_AIRSPEED_SETPOINT"/>
        <dl_setting max="2" min="0" step="0.01" var="v_ctl_auto_airspeed_pgain" shortname="as_t_pgain" param="V_CTL_AUTO_AIRSPEED_PGAIN"/>
        <dl_setting max="2" min="0" step="0.01" var="v_ctl_auto_airspeed_igain" shortname="as_t_igain" param="V_CTL_AUTO_AIRSPEED_IGAIN"/>
        <dl_setting max="40" min="5" step="0.1" var="v_ctl_auto_groundspeed_setpoint" shortname="gs_sp" param="V_CTL_AUTO_GROUNDSPEED_SETPOINT"/>
        <dl_setting max="2" min="0" step="0.01" var="v_ctl_auto_groundspeed_pgain" shortname="gs_pgain" param="V_CTL_AUTO_GROUNDSPEED_PGAIN"/>
        <dl_setting max="2" min="0" step="0.01" var="v_ctl_auto_groundspeed_igain" shortname="gs_igain" param="V_CTL_AUTO_GROUNDSPEED_IGAIN"/>
      </dl_settings>
  </dl_settings>
</settings>


